/* pages/list/list.wxss */
.banner{
  margin-bottom: 20rpx;
}
.banner image{
   width: 100%;
   height: 400rpx;
}
.new-box{
  padding: 0 40rpx;
}
.box-item{
  display: inline-block;
  width: 325rpx;
  height: 325rpx;
  margin:0 20rpx 20rpx 0;
  border-radius: 10px;
  text-align: center;
  background: #f5f6f5;
}
.box-item:nth-child(2n){
  margin-right: 0;
}
.box-item image{
   width: 175rpx;
   height: 175rpx;
   margin: 20rpx 0 10rpx;
}